{
  "name": "lui-icon-solid",
  "description": "A LUI-icon icon library package for Solid applications.",
  "version": "0.0.1",
  "license": "ISC",
  "homepage": "https://lui-icon.dev",
  "bugs": "https://github.com/lui-icon/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/lui-icon.git",
    "directory": "packages/lui-icon-solid"
  },
  "keywords": [
    "LUI-icon",
    "Solid",
    "SolidJS",
    "Feather",
    "Icons",
    "Icon",
    "SVG",
    "Feather Icons",
    "Fontawesome",
    "Font Awesome"
  ],
  "author": "Eric Fennis",
  "source": "src/lui-icon-solid.ts",
  "main": "dist/cjs/lui-icon-solid.js",
  "module": "dist/esm/lui-icon-solid.js",
  "types": "dist/types/lui-icon-solid.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/lui-icon-solid.d.ts",
      "solid": "./dist/source/lui-icon-solid.jsx",
      "import": "./dist/esm/lui-icon-solid.js",
      "browser": "./dist/esm/lui-icon-solid.js",
      "require": "./dist/cjs/lui-icon-solid.js"
    },
    "./icons": {
      "types": "./dist/types/lui-icon-solid.d.ts",
      "solid": "./dist/source/lui-icon-solid.jsx",
      "import": "./dist/esm/lui-icon-solid.js",
      "browser": "./dist/esm/lui-icon-solid.js",
      "require": "./dist/cjs/lui-icon-solid.js"
    },
    "./icons/*": {
      "types": "./dist/types/icons/*.d.ts",
      "solid": "./dist/source/icons/*.jsx",
      "import": "./dist/esm/icons/*.js",
      "browser": "./dist/esm/icons/*.js",
      "require": "./dist/cjs/icons/*.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "pnpm clean && pnpm copy:license && pnpm build:icons && pnpm build:bundle",
    "copy:license": "cp ../../LICENSE ./LICENSE",
    "clean": "rm -rf dist && rm -rf stats && rm -rf ./src/icons/*.ts",
    "build:transpile": "tsc --jsx preserve -t es2020 --rootDir src --outDir dist --noEmit false",
    "build:version": "node ./scripts/replaceVersion.mjs",
    "build:bundle": "rollup -c rollup.config.mjs",
    "build:icons": "build-icons --output=./src --templateSrc=./scripts/exportTemplate.mts --renderUniqueKey --withAliases --separateAliasesFile --separateAliasesFileIgnore=fingerprint --aliasesFileExtension=.ts --iconFileExtension=.tsx --exportFileName=index.ts",
    "test": "pnpm build:icons && vitest run",
    "test:watch": "vitest watch",
    "version": "pnpm version --git-tag-version=false"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-typescript": "^7.23.3",
    "@lui-icon/build-icons": "workspace:*",
    "@lui-icon/rollup-plugins": "workspace:*",
    "@lui-icon/shared": "workspace:*",
    "@rollup/plugin-babel": "^6.0.4",
    "@solidjs/testing-library": "^0.8.10",
    "@testing-library/jest-dom": "^6.8.0",
    "babel-preset-solid": "^1.8.12",
    "jest-serializer-html": "^7.1.0",
    "rollup": "^4.53.3",
    "solid-js": "^1.9.4",
    "typescript": "^5.8.3",
    "vite": "^7.2.4",
    "vite-plugin-solid": "^2.11.6",
    "vitest": "^4.0.12",
    "esbuild": "^0.25.0"
  },
  "peerDependencies": {
    "solid-js": "^1.4.7"
  }
}
