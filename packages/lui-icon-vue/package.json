{
  "name": "lui-icon-vue",
  "version": "0.0.1",
  "author": "Eric Fennis",
  "description": "A LUI-icon library package for Vue 2 applications.",
  "license": "ISC",
  "homepage": "https://lui-icon.dev",
  "bugs": "https://github.com/lui-icon-icons/lucide/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/lui-icon-icons/lucide.git",
    "directory": "packages/lui-icon-vue"
  },
  "keywords": [
    "LUI-icon",
    "Vue",
    "Feather",
    "Icons",
    "Icon",
    "SVG",
    "Feather Icons",
    "Fontawesome",
    "Font Awesome"
  ],
  "amdName": "lui-icon-vue",
  "source": "build/lui-icon-vue.js",
  "main": "dist/cjs/lui-icon-vue.js",
  "main:umd": "dist/umd/lui-icon-vue.js",
  "module": "dist/esm/lui-icon-vue.js",
  "unpkg": "dist/umd/lui-icon-vue.min.js",
  "sideEffects": false,
  "files": [
    "dist",
    "nuxt.js"
  ],
  "scripts": {
    "build": "pnpm clean && pnpm copy:license && pnpm build:icons && pnpm build:bundles",
    "copy:license": "cp ../../LICENSE ./LICENSE",
    "clean": "rm -rf dist && rm -rf stats && rm -rf ./src/icons/*.js",
    "build:icons": "build-icons --output=./src --templateSrc=./scripts/exportTemplate.mjs --renderUniqueKey --withAliases --aliasesFileExtension=.ts --iconFileExtension=.ts --exportFileName=index.ts",
    "build:bundles": "rollup -c ./rollup.config.mjs",
    "test": "pnpm build:icons && vitest run",
    "version": "pnpm version --git-tag-version=false"
  },
  "devDependencies": {
    "@lui-icon/build-icons": "workspace:*",
    "@lui-icon/rollup-plugins": "workspace:*",
    "@lui-icon/shared": "workspace:*",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/vue": "^5.9.0",
    "@vitejs/plugin-vue2": "2.2.0",
    "@vue/test-utils": "1.3.0",
    "rollup": "^4.53.3",
    "typescript": "^5.8.3",
    "vite": "^7.2.4",
    "vitest": "^4.0.12",
    "vue": "2.7.14",
    "vue-template-compiler": "2.7.14"
  },
  "peerDependencies": {
    "vue": "^2.6.12"
  }
}
