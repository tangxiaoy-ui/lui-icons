#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ottopan/Documents/coder/tubiao/tools/build-icons/node_modules:/Users/ottopan/Documents/coder/tubiao/tools/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules:/Users/ottopan/Documents/coder/node_modules:/Users/ottopan/Documents/node_modules:/Users/ottopan/node_modules:/Users/node_modules:/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ottopan/Documents/coder/tubiao/tools/build-icons/node_modules:/Users/ottopan/Documents/coder/tubiao/tools/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules:/Users/ottopan/Documents/coder/node_modules:/Users/ottopan/Documents/node_modules:/Users/ottopan/node_modules:/Users/node_modules:/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@lui-icon/build-icons/cli.ts" "$@"
else
  exec node  "$basedir/../@lui-icon/build-icons/cli.ts" "$@"
fi
