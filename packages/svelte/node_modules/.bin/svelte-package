#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/@sveltejs+package@2.5.6_svelte@5.43.14_typescript@5.9.3/node_modules/@sveltejs/package/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/@sveltejs+package@2.5.6_svelte@5.43.14_typescript@5.9.3/node_modules/@sveltejs/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/@sveltejs+package@2.5.6_svelte@5.43.14_typescript@5.9.3/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/@sveltejs+package@2.5.6_svelte@5.43.14_typescript@5.9.3/node_modules/@sveltejs/package/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/@sveltejs+package@2.5.6_svelte@5.43.14_typescript@5.9.3/node_modules/@sveltejs/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/@sveltejs+package@2.5.6_svelte@5.43.14_typescript@5.9.3/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@sveltejs/package/svelte-package.js" "$@"
else
  exec node  "$basedir/../@sveltejs/package/svelte-package.js" "$@"
fi
