#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/ajv-cli@5.0.0_ts-node@10.9.2_@swc+core@1.7.23_@swc+helpers@0.5.17__@types+node@24.10.1_typescript@5.9.3_/node_modules/ajv-cli/dist/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/ajv-cli@5.0.0_ts-node@10.9.2_@swc+core@1.7.23_@swc+helpers@0.5.17__@types+node@24.10.1_typescript@5.9.3_/node_modules/ajv-cli/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/ajv-cli@5.0.0_ts-node@10.9.2_@swc+core@1.7.23_@swc+helpers@0.5.17__@types+node@24.10.1_typescript@5.9.3_/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/ajv-cli@5.0.0_ts-node@10.9.2_@swc+core@1.7.23_@swc+helpers@0.5.17__@types+node@24.10.1_typescript@5.9.3_/node_modules/ajv-cli/dist/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/ajv-cli@5.0.0_ts-node@10.9.2_@swc+core@1.7.23_@swc+helpers@0.5.17__@types+node@24.10.1_typescript@5.9.3_/node_modules/ajv-cli/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/ajv-cli@5.0.0_ts-node@10.9.2_@swc+core@1.7.23_@swc+helpers@0.5.17__@types+node@24.10.1_typescript@5.9.3_/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ajv-cli/dist/index.js" "$@"
else
  exec node  "$basedir/../ajv-cli/dist/index.js" "$@"
fi
