#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/vitest@4.0.12_@types+debug@4.1.12_@types+node@24.10.1_jiti@2.6.1_jsdom@20.0.3_less@4.2._db2bcaeb98a84c7c619ba5b83e37172d/node_modules/vitest/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/vitest@4.0.12_@types+debug@4.1.12_@types+node@24.10.1_jiti@2.6.1_jsdom@20.0.3_less@4.2._db2bcaeb98a84c7c619ba5b83e37172d/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/vitest@4.0.12_@types+debug@4.1.12_@types+node@24.10.1_jiti@2.6.1_jsdom@20.0.3_less@4.2._db2bcaeb98a84c7c619ba5b83e37172d/node_modules/vitest/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/vitest@4.0.12_@types+debug@4.1.12_@types+node@24.10.1_jiti@2.6.1_jsdom@20.0.3_less@4.2._db2bcaeb98a84c7c619ba5b83e37172d/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../vitest.mjs" "$@"
else
  exec node  "$basedir/../../vitest.mjs" "$@"
fi
