#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1.7.23_@swc+helpers@0.5.17__@types+node@12.20.55_typescript@4.6.4/node_modules/ts-node/dist/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1.7.23_@swc+helpers@0.5.17__@types+node@12.20.55_typescript@4.6.4/node_modules/ts-node/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1.7.23_@swc+helpers@0.5.17__@types+node@12.20.55_typescript@4.6.4/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1.7.23_@swc+helpers@0.5.17__@types+node@12.20.55_typescript@4.6.4/node_modules/ts-node/dist/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1.7.23_@swc+helpers@0.5.17__@types+node@12.20.55_typescript@4.6.4/node_modules/ts-node/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/ts-node@10.9.2_@swc+core@1.7.23_@swc+helpers@0.5.17__@types+node@12.20.55_typescript@4.6.4/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/bin.js" "$@"
else
  exec node  "$basedir/../../dist/bin.js" "$@"
fi
