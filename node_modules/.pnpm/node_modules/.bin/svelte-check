#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/svelte-check@3.8.6_@babel+core@7.28.5_less@4.2.0_postcss@8.5.6_sass@1.77.8_svelte@4.2.20/node_modules/svelte-check/bin/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/svelte-check@3.8.6_@babel+core@7.28.5_less@4.2.0_postcss@8.5.6_sass@1.77.8_svelte@4.2.20/node_modules/svelte-check/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/svelte-check@3.8.6_@babel+core@7.28.5_less@4.2.0_postcss@8.5.6_sass@1.77.8_svelte@4.2.20/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/svelte-check@3.8.6_@babel+core@7.28.5_less@4.2.0_postcss@8.5.6_sass@1.77.8_svelte@4.2.20/node_modules/svelte-check/bin/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/svelte-check@3.8.6_@babel+core@7.28.5_less@4.2.0_postcss@8.5.6_sass@1.77.8_svelte@4.2.20/node_modules/svelte-check/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/svelte-check@3.8.6_@babel+core@7.28.5_less@4.2.0_postcss@8.5.6_sass@1.77.8_svelte@4.2.20/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../svelte-check/bin/svelte-check" "$@"
else
  exec node  "$basedir/../svelte-check/bin/svelte-check" "$@"
fi
