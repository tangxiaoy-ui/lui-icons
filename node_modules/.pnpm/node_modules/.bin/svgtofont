#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/svgtofont@6.5.0_@types+svg2ttf@5.0.1_chokidar@3.6.0/node_modules/svgtofont/lib/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/svgtofont@6.5.0_@types+svg2ttf@5.0.1_chokidar@3.6.0/node_modules/svgtofont/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/svgtofont@6.5.0_@types+svg2ttf@5.0.1_chokidar@3.6.0/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/svgtofont@6.5.0_@types+svg2ttf@5.0.1_chokidar@3.6.0/node_modules/svgtofont/lib/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/svgtofont@6.5.0_@types+svg2ttf@5.0.1_chokidar@3.6.0/node_modules/svgtofont/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/svgtofont@6.5.0_@types+svg2ttf@5.0.1_chokidar@3.6.0/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../svgtofont/lib/cli.js" "$@"
else
  exec node  "$basedir/../svgtofont/lib/cli.js" "$@"
fi
