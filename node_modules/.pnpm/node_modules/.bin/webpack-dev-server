#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/webpack-dev-server@4.7.3_@types+express@4.17.21_webpack@5.76.1_@swc+core@1.7.23_@swc+helpers@0.5.17__esbuild@0.14.22_/node_modules/webpack-dev-server/bin/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/webpack-dev-server@4.7.3_@types+express@4.17.21_webpack@5.76.1_@swc+core@1.7.23_@swc+helpers@0.5.17__esbuild@0.14.22_/node_modules/webpack-dev-server/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/webpack-dev-server@4.7.3_@types+express@4.17.21_webpack@5.76.1_@swc+core@1.7.23_@swc+helpers@0.5.17__esbuild@0.14.22_/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/webpack-dev-server@4.7.3_@types+express@4.17.21_webpack@5.76.1_@swc+core@1.7.23_@swc+helpers@0.5.17__esbuild@0.14.22_/node_modules/webpack-dev-server/bin/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/webpack-dev-server@4.7.3_@types+express@4.17.21_webpack@5.76.1_@swc+core@1.7.23_@swc+helpers@0.5.17__esbuild@0.14.22_/node_modules/webpack-dev-server/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/webpack-dev-server@4.7.3_@types+express@4.17.21_webpack@5.76.1_@swc+core@1.7.23_@swc+helpers@0.5.17__esbuild@0.14.22_/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../webpack-dev-server/bin/webpack-dev-server.js" "$@"
else
  exec node  "$basedir/../webpack-dev-server/bin/webpack-dev-server.js" "$@"
fi
