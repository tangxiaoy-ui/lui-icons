#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/@angular+compiler-cli@13.3.12_@angular+compiler@13.3.12_typescript@4.6.4/node_modules/@angular/compiler-cli/bundles/ngcc/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/@angular+compiler-cli@13.3.12_@angular+compiler@13.3.12_typescript@4.6.4/node_modules/@angular/compiler-cli/bundles/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/@angular+compiler-cli@13.3.12_@angular+compiler@13.3.12_typescript@4.6.4/node_modules/@angular/compiler-cli/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/@angular+compiler-cli@13.3.12_@angular+compiler@13.3.12_typescript@4.6.4/node_modules/@angular/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/@angular+compiler-cli@13.3.12_@angular+compiler@13.3.12_typescript@4.6.4/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/@angular+compiler-cli@13.3.12_@angular+compiler@13.3.12_typescript@4.6.4/node_modules/@angular/compiler-cli/bundles/ngcc/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/@angular+compiler-cli@13.3.12_@angular+compiler@13.3.12_typescript@4.6.4/node_modules/@angular/compiler-cli/bundles/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/@angular+compiler-cli@13.3.12_@angular+compiler@13.3.12_typescript@4.6.4/node_modules/@angular/compiler-cli/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/@angular+compiler-cli@13.3.12_@angular+compiler@13.3.12_typescript@4.6.4/node_modules/@angular/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/@angular+compiler-cli@13.3.12_@angular+compiler@13.3.12_typescript@4.6.4/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@angular/compiler-cli/bundles/ngcc/main-ngcc.js" "$@"
else
  exec node  "$basedir/../@angular/compiler-cli/bundles/ngcc/main-ngcc.js" "$@"
fi
