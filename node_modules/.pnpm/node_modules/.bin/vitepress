#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.44.0_@types+node@24.10.1_@types+react@18.3.27__bd12e4ed161d6807b9c5c2348512a125/node_modules/vitepress/bin/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.44.0_@types+node@24.10.1_@types+react@18.3.27__bd12e4ed161d6807b9c5c2348512a125/node_modules/vitepress/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.44.0_@types+node@24.10.1_@types+react@18.3.27__bd12e4ed161d6807b9c5c2348512a125/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.44.0_@types+node@24.10.1_@types+react@18.3.27__bd12e4ed161d6807b9c5c2348512a125/node_modules/vitepress/bin/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.44.0_@types+node@24.10.1_@types+react@18.3.27__bd12e4ed161d6807b9c5c2348512a125/node_modules/vitepress/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.44.0_@types+node@24.10.1_@types+react@18.3.27__bd12e4ed161d6807b9c5c2348512a125/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitepress/bin/vitepress.js" "$@"
else
  exec node  "$basedir/../vitepress/bin/vitepress.js" "$@"
fi
