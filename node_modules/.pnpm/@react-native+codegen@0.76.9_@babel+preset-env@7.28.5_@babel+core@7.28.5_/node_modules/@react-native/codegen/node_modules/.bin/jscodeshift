#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/jscodeshift@0.14.0_@babel+preset-env@7.28.5_@babel+core@7.28.5_/node_modules/jscodeshift/bin/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/jscodeshift@0.14.0_@babel+preset-env@7.28.5_@babel+core@7.28.5_/node_modules/jscodeshift/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/jscodeshift@0.14.0_@babel+preset-env@7.28.5_@babel+core@7.28.5_/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/jscodeshift@0.14.0_@babel+preset-env@7.28.5_@babel+core@7.28.5_/node_modules/jscodeshift/bin/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/jscodeshift@0.14.0_@babel+preset-env@7.28.5_@babel+core@7.28.5_/node_modules/jscodeshift/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/jscodeshift@0.14.0_@babel+preset-env@7.28.5_@babel+core@7.28.5_/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../jscodeshift@0.14.0_@babel+preset-env@7.28.5_@babel+core@7.28.5_/node_modules/jscodeshift/bin/jscodeshift.js" "$@"
else
  exec node  "$basedir/../../../../../../jscodeshift@0.14.0_@babel+preset-env@7.28.5_@babel+core@7.28.5_/node_modules/jscodeshift/bin/jscodeshift.js" "$@"
fi
