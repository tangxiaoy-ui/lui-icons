#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/astro@5.16.0_@types+node@24.10.1_db0@0.3.4_ioredis@5.8.2_jiti@2.6.1_less@4.2.0_rollup@4_a9b867b91918e6c627e92bf53704a395/node_modules/astro/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/astro@5.16.0_@types+node@24.10.1_db0@0.3.4_ioredis@5.8.2_jiti@2.6.1_less@4.2.0_rollup@4_a9b867b91918e6c627e92bf53704a395/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/astro@5.16.0_@types+node@24.10.1_db0@0.3.4_ioredis@5.8.2_jiti@2.6.1_less@4.2.0_rollup@4_a9b867b91918e6c627e92bf53704a395/node_modules/astro/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/astro@5.16.0_@types+node@24.10.1_db0@0.3.4_ioredis@5.8.2_jiti@2.6.1_less@4.2.0_rollup@4_a9b867b91918e6c627e92bf53704a395/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../astro.js" "$@"
else
  exec node  "$basedir/../../astro.js" "$@"
fi
