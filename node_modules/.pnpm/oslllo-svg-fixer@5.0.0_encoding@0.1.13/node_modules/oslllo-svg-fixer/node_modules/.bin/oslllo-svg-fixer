#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/oslllo-svg-fixer@5.0.0_encoding@0.1.13/node_modules/oslllo-svg-fixer/src/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/oslllo-svg-fixer@5.0.0_encoding@0.1.13/node_modules/oslllo-svg-fixer/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/oslllo-svg-fixer@5.0.0_encoding@0.1.13/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/oslllo-svg-fixer@5.0.0_encoding@0.1.13/node_modules/oslllo-svg-fixer/src/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/oslllo-svg-fixer@5.0.0_encoding@0.1.13/node_modules/oslllo-svg-fixer/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/oslllo-svg-fixer@5.0.0_encoding@0.1.13/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../src/cli.js" "$@"
else
  exec node  "$basedir/../../src/cli.js" "$@"
fi
