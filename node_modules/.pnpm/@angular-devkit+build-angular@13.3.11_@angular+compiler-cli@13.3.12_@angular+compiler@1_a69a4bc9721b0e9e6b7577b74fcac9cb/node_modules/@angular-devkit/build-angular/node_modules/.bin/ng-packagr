#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/ng-packagr@13.3.1_@angular+compiler-cli@13.3.12_@angular+compiler@13.3.12_typescript@4._206cace6211aa49e2aece525473efa9a/node_modules/ng-packagr/cli/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/ng-packagr@13.3.1_@angular+compiler-cli@13.3.12_@angular+compiler@13.3.12_typescript@4._206cace6211aa49e2aece525473efa9a/node_modules/ng-packagr/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/ng-packagr@13.3.1_@angular+compiler-cli@13.3.12_@angular+compiler@13.3.12_typescript@4._206cace6211aa49e2aece525473efa9a/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/ng-packagr@13.3.1_@angular+compiler-cli@13.3.12_@angular+compiler@13.3.12_typescript@4._206cace6211aa49e2aece525473efa9a/node_modules/ng-packagr/cli/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/ng-packagr@13.3.1_@angular+compiler-cli@13.3.12_@angular+compiler@13.3.12_typescript@4._206cace6211aa49e2aece525473efa9a/node_modules/ng-packagr/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/ng-packagr@13.3.1_@angular+compiler-cli@13.3.12_@angular+compiler@13.3.12_typescript@4._206cace6211aa49e2aece525473efa9a/node_modules:/Users/ottopan/Documents/coder/tubiao/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../ng-packagr@13.3.1_@angular+compiler-cli@13.3.12_@angular+compiler@13.3.12_typescript@4._206cace6211aa49e2aece525473efa9a/node_modules/ng-packagr/cli/main.js" "$@"
else
  exec node  "$basedir/../../../../../../ng-packagr@13.3.1_@angular+compiler-cli@13.3.12_@angular+compiler@13.3.12_typescript@4._206cace6211aa49e2aece525473efa9a/node_modules/ng-packagr/cli/main.js" "$@"
fi
